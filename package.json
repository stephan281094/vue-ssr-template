{
  "name": "vue-ssr-template",
  "description": "A Vue 2.0 server side rendering template",
  "author": "Stephan de Vries <stephan281094@gmail.com>",
  "private": false,
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack.server.config.js --progress --hide-modules",
    "lint": "standard",
    "test": "yarn run lint"
  },
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "cross-env": "^3.1.3",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "lru-cache": "^4.0.2",
    "serialize-javascript": "^1.3.0",
    "vue": "^2.1.8",
    "vue-meta": "^0.5.3",
    "vue-router": "^2.1.0",
    "vue-server-renderer": "^2.1.8",
    "vuex": "^2.1.0",
    "vuex-router-sync": "^4.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "buble": "^0.15.1",
    "buble-loader": "^0.4.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^4.2.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "standard": "^8.6.0",
    "sw-precache-webpack-plugin": "^0.7.0",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.1.8",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  }
}
