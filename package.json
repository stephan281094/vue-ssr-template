{
  "name": "vue-ssr-template",
  "description": "A Vue 2.0 server side rendering template",
  "author": "Stephan de Vries <stephan281094@gmail.com>",
  "private": false,
  "license": "MIT",
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "lint": "standard",
    "test": "cross-env karma start tests/unit/karma.conf.js --single-run"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "cross-env": "^3.2.4",
    "es6-promise": "^4.1.0",
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "lru-cache": "^4.0.2",
    "vue": "^2.2.4",
    "vue-router": "^2.3.0",
    "vue-server-renderer": "^2.2.4",
    "vuex": "^2.2.1",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "buble": "^0.15.2",
    "buble-loader": "^0.4.1",
    "chai": "^3.5.0",
    "css-loader": "^0.27.3",
    "file-loader": "^0.10.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.28.0",
    "karma": "^1.5.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "node-sass": "^4.2.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "standard": "^9.0.1",
    "sw-precache-webpack-plugin": "^0.9.1",
    "url-loader": "^0.5.8",
    "vue-loader": "^11.1.4",
    "vue-ssr-webpack-plugin": "^1.0.2",
    "vue-template-compiler": "^2.2.4",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^4.0.0"
  }
}
